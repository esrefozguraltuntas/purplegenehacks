{
  "name": "ChromYQLip",
  "version": "2.6",
  "description": "Quickly clip any information from a page & ignore the noise. Targeting web developers for YQL scrapping.",
  "icons":{"128":"icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "ChromYQLip",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": [
    "http://*/*",
     "tabs"
  ]
}
